    "@remkoj/optimizely-graph-client": "file:./packages/@remkoj-optimizely-graph-client-dev.tgz",
    "@remkoj/optimizely-one-nextjs": "file:./packages/@remkoj-optimizely-one-nextjs-dev.tgz",
    "@remkoj/optimizely-cms-api": "file:./packages/@remkoj-optimizely-cms-api-dev.tgz",
    "@remkoj/optimizely-cms-cli": "file:./packages/@remkoj-optimizely-cms-cli-dev.tgz",
    "@remkoj/optimizely-graph-cli": "file:./packages/@remkoj-optimizely-graph-cli-dev.tgz",
    "@remkoj/optimizely-graph-functions": "file:./packages/@remkoj-optimizely-graph-functions-dev.tgz"
  }
